{
  "name": "@wesp-up/checkit-app",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "cross-env NODE_ENV=production run-s clean build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "build:server": "esbuild `find server \\( -name '*.ts' \\)` --platform=node --sourcemap --format=cjs --log-level=warning --outdir=build --tsconfig=server/tsconfig.json",
    "ci": "run-p lint typecheck test:coverage build",
    "clean": "rm -rf logs coverage build public/build app/styles/tailwind.css",
    "dev": "run-s clean && run-s build:css build:remix && run-p dev:*",
    "dev:css": "cross-env NODE_ENV=development npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development REMIX_DEV_SERVER_WS_PORT=8002 remix watch",
    "dev:server": "cross-env NODE_ENV=development SESSION_SECRET=abc123 nodemon --watch server -e ts --ignore '*.test.ts' --exec 'npm run build:server && node --require dotenv/config ./build/server.js'",
    "docker:build": "cd ../.. && SCOPE=@wesp-up/checkit-app APP_NAME=checkit-app .ci/app/build.sh",
    "docker:deploy": "cd ../.. && APP_NAME=checkit-app WORKSPACE_PATH=apps/checkit-app REMOTE_APP_DIRECTORY=checkit .ci/app/deploy.sh",
    "docker:run": "cd ../.. && APP_NAME=checkit-app .ci/app/run.sh yarn workspace @wesp-up/checkit-app start",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "npm run lint -- --fix",
    "start": "cross-env NODE_ENV=production node --require dotenv/config ./build/server.js",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "typecheck": "tsc -b"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.6",
    "@reach/checkbox": "^0.17.0",
    "@reach/dialog": "^0.17.0",
    "@reach/menu-button": "^0.17.0",
    "@reach/tooltip": "^0.17.0",
    "@reach/utils": "^0.17.0",
    "@remix-run/express": "^1.6.5",
    "@remix-run/node": "^1.6.5",
    "@remix-run/react": "^1.6.5",
    "@remix-run/serve": "^1.6.5",
    "@wesp-up/sample-test": "*",
    "@wesp-up/ui": "*",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.3.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-prom-bundle": "^6.5.0",
    "mongodb": "^4.7.0",
    "morgan": "^1.10.0",
    "prom-client": "^14.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix-auth": "^3.2.2",
    "remix-auth-form": "^1.1.2",
    "remix-auth-google": "^1.1.0",
    "rotating-file-stream": "^3.0.4",
    "tiny-invariant": "^1.2.0",
    "uuid": "^8.3.2",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.6.5",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/dom": "^8.14.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.2.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.4",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.0.1",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.6",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-react": "^1.3.2",
    "@wesp-up/eslint-config": "*",
    "@wesp-up/tsconfig": "*",
    "autoprefixer": "^10.4.7",
    "binode": "^1.0.5",
    "c8": "^7.11.3",
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.48",
    "eslint": "^8.19.0",
    "happy-dom": "^6.0.2",
    "msw": "^0.43.0",
    "nodemon": "^2.0.18",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.4",
    "ts-node": "^10.8.2",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.13",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.17.0"
  }
}
