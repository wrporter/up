{
  "name": "@wesp-up/checkit-app",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "ignore:build": "cross-env NODE_ENV=production run-s clean build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "build:server": "esbuild `find server \\( -name '*.ts' \\)` --platform=node --sourcemap --format=cjs --log-level=warning --outdir=build --tsconfig=server/tsconfig.json",
    "ignore:ci": "run-p lint typecheck test:coverage build",
    "ignore:clean": "rm -rf logs coverage build public/build app/styles/tailwind.css",
    "ignore:dev": "run-s clean && run-s build:css build:remix && run-p dev:*",
    "dev:css": "cross-env NODE_ENV=development npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development REMIX_DEV_SERVER_WS_PORT=8002 remix watch",
    "dev:server": "cross-env NODE_ENV=development SESSION_SECRET=abc123 nodemon --watch server -e ts --ignore '*.test.ts' --exec 'npm run build:server && node --require dotenv/config ./build/server.js'",
    "ignore:docker:build": "cd ../.. && SCOPE=@wesp-up/checkit-app APP_NAME=checkit-app .ci/app/build.sh",
    "ignore:docker:deploy": "cd ../.. && APP_NAME=checkit-app WORKSPACE_PATH=apps/checkit-app REMOTE_APP_DIRECTORY=checkit .ci/app/deploy.sh",
    "ignore:docker:run": "cd ../.. && APP_NAME=checkit-app .ci/app/run.sh yarn workspace @wesp-up/checkit-app start",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "ignore:lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "ignore:lint:fix": "npm run lint -- --fix",
    "start": "cross-env NODE_ENV=production node --require dotenv/config ./build/server.js",
    "ignore:test": "vitest run",
    "ignore:test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "ignore:typecheck": "tsc -b"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@remix-run/express": "^2.3.1",
    "@remix-run/node": "^2.3.1",
    "@remix-run/react": "^2.3.1",
    "@remix-run/serve": "^2.3.1",
    "@wesp-up/ui": "*",
    "bcryptjs": "^2.4.3",
    "classnames": "^2.3.2",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-prom-bundle": "^6.6.0",
    "mongodb": "^6.3.0",
    "morgan": "^1.10.0",
    "prom-client": "^15.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.6.0",
    "remix-auth-form": "^1.4.0",
    "remix-auth-google": "^2.0.0",
    "rotating-file-stream": "^3.1.1",
    "tiny-invariant": "^1.3.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.3.1",
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.10",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "@types/uuid": "^9.0.7",
    "@vitejs/plugin-react": "^4.2.0",
    "@wesp-up/eslint-config-react": "*",
    "@wesp-up/tsconfig": "*",
    "autoprefixer": "^10.4.16",
    "binode": "^1.0.5",
    "c8": "^8.0.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.19.7",
    "eslint": "^8.54.0",
    "happy-dom": "^12.10.3",
    "msw": "^2.0.8",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2",
    "vite": "^5.0.2",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  }
}
