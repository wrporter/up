{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "docker:build": {
      "outputs": []
    },
    "docker:deploy": {
      "dependsOn": ["docker:build"],
      "outputs": []
    },
    "docs": {
      "dependsOn": ["^build"],
      "outputs": ["docs"]
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "lint": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "lint:fix": {
      "cache": false,
      "outputs": []
    },
    "publish": {
      "dependsOn": ["build"]
    },
    "typecheck": {
      "dependsOn": ["build"],
      "outputs": []
    }
  },
  "remoteCache": {
    "signature": true
  }
}
