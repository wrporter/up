{
  "name": "@wesp-up/express",
  "version": "3.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/wrporter/up.git"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "files": [
    "README.md",
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "ci": "npm run lint && npm run typecheck && npm run build && npm run test && npm run docs",
    "dev": "tsup --watch",
    "clean": "npx rimraf .turbo dist node_modules",
    "docs": "typedoc",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "typecheck": "tsc -b"
  },
  "peerDependencies": {
    "express": ">=4.18.2",
    "prom-client": ">=14.2.0"
  },
  "dependencies": {
    "@wesp-up/logger": "*",
    "@wesp-up/metrics": "*",
    "compression": "1.7.5",
    "express-prom-bundle": "7.0.2",
    "http-errors": "2.0.0",
    "http-terminator": "3.2.0",
    "on-finished": "2.4.1",
    "statuses": "2.0.1",
    "uuid": "11.0.2"
  },
  "devDependencies": {
    "@types/compression": "1.7.5",
    "@types/express": "4.17.21",
    "@types/http-errors": "2.0.4",
    "@types/node": "22.8.5",
    "@types/on-finished": "2.3.4",
    "@types/statuses": "2.0.5",
    "@types/supertest": "6.0.2",
    "@types/uuid": "10.0.0",
    "@vitest/coverage-v8": "2.1.4",
    "@wesp-up/eslint-config": "*",
    "@wesp-up/tsconfig": "*",
    "@wesp-up/typedoc-config": "*",
    "eslint": "9.13.0",
    "express": "4.21.1",
    "prom-client": "15.1.3",
    "supertest": "7.0.0",
    "tsup": "8.3.5",
    "typescript": "5.6.3",
    "vite": "5.4.10",
    "vite-tsconfig-paths": "5.0.1",
    "vitest": "2.1.4"
  }
}
